{
    "name": "hexide-digital/gitlab-deploy",
    "description": "Prepare your project to deploy",
    "type": "library",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0",
        "graham-campbell/gitlab": "^5.6",
        "guzzlehttp/guzzle": "^7.4",
        "http-interop/http-factory-guzzle": "^1.0"
    },
    "require-dev": {
        "deployer/deployer": "^7.0"
    },
    "conflict": {
        "deployer/deployer": "<7.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "HexideDigital\\GitlabDeploy\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Oleksandr-Moik",
            "email": "sasha.moik.2016@gmail.com",
            "homepage": "https://github.com/Oleksandr-Moik",
            "role": "developer"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "HexideDigital\\GitlabDeploy\\GitlabDeployServiceProvider"
            ]
        }
    }
}
